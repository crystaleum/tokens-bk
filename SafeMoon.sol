    function checkFees(FeeTier memory _tier) internal view returns (FeeTier memory) {
        uint256 _fees = _tier.marketingFee.add(_tier.liquidityFee).add(_tier.taxFee).add(_tier.burnFee);
        require(_fees <= _maxFee, "Safemoon: Fees exceeded max limitation");

        return _tier;
    }

    function checkFeesChanged(FeeTier memory _tier, uint256 _oldFee, uint256 _newFee) internal view {
        uint256 _fees = _tier.marketingFee
        .add(_tier.liquidityFee)
        .add(_tier.taxFee)
        .add(_tier.burnFee)
        .sub(_oldFee)
        .add(_newFee);

        require(_fees <= _maxFee, "Safemoon: Fees exceeded max limitation");
    }

    function setMarketingFeePercent(uint256 _tierIndex, uint256 _marketingFee) external onlyOwner() checkTierIndex(_tierIndex) {
        FeeTier memory tier = feeTiers[_tierIndex];
